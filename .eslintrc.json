{
  "extends": ["eslint:recommended"],
  "plugins": [],
  "parserOptions": { "sourceType": "module" },
  "env": { "browser": true, "es2020": true },
  "globals": {
    "process": false
  },
  "rules": {
    "quotes": ["warn", "single"],
    "no-unused-vars": ["warn", {
      "varsIgnorePattern": "^_",
      "argsIgnorePattern": "^_"
    }]
  }
}
